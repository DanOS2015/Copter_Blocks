{
  "name": "CopterBlocks",
  "version": "1.0.0",
  "description": "Copter Blocks. A graphical programming environment for programming a quadcopter.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "repository": "https://gitlab.computing.dcu.ie/geoghen4/2017-CA326-ngeoghegan-copterblocks.git",
  "keywords": [
    "copter blocks",
    "quadcopter",
    "programming",
    "graphical",
    "drone"
  ],
  "author": "Daniel O'Sullivan and Neil Geoghegan",
  "license": "CC0-1.0",
  "devDependencies": {
    "build-electron-app": "github:j-f1/build-electron-app",
    "electron": "^1.4.1",
    "electron-packager": "^8.5.2",
    "electron-rebuild": "^1.5.7"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "electron-osx-sign": "^0.4.4",
    "threads": "^0.7.2"
  }
}
